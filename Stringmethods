public class Stringmethods {

	public static void main(String[] args) {
		StringBuffer sb = new StringBuffer("Hello");
         System.out.println(sb);
         sb.append(" World");
         System.out.println(sb);
         sb.insert(5," Java");
         System.out.println(sb);
         sb.delete(5, 10);
         System.out.println(sb);
	}

}
